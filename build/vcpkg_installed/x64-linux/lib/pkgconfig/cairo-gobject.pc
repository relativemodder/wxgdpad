prefix=${pcfiledir}/../..
includedir=${prefix}/include
libdir=${prefix}/lib

Name: cairo-gobject
Description: cairo-gobject for cairo graphics library
Version: 1.18.2

Libs: "-L${libdir}" -lcairo-gobject -lm -ldl
Requires: cairo, glib-2.0 >=  2.14, gobject-2.0, zlib, libpng >=  1.4.0, fontconfig >=  2.13.0, freetype2 >=  23.0.17, x11, xext, xrender >=  0.6, pixman-1 >=  0.40.0
Cflags: "-I${includedir}/cairo"
