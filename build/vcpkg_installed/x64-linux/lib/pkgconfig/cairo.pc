prefix=${pcfiledir}/../..
includedir=${prefix}/include
libdir=${prefix}/lib

Name: cairo
Description: Multi-platform 2D graphics library
Version: 1.18.2

Libs: "-L${libdir}" -lcairo -lm -ldl
Requires: zlib, libpng >=  1.4.0, fontconfig >=  2.13.0, freetype2 >=  23.0.17, x11, xext, xrender >=  0.6, pixman-1 >=  0.40.0
Cflags: "-I${includedir}/cairo"
